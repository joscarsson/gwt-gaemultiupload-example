<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:h="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.shell {
			padding: 5px;
		}
	</ui:style>
	<g:LayoutPanel>
		<g:layer>
			<g:HTMLPanel styleName="{style.shell}">
				<g:Button text="Browse" ui:field="btnBrowse" />
				<g:Button text="Start Upload" ui:field="btnStart" /><br />
				<br />
				<h:CellTable width="600px" ui:field="tblFiles" />
			</g:HTMLPanel>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 